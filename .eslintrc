{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": false
  },
  "rules": {
     "semi": "off",
     "linebreak-style": ["error", "windows"],
     "comma-dangle": "off",
     "indent": "off",
     "no-undef": "off"
  },
  "extends": ["eslint:recommended", "google"]
}